    {
      "cell_type": "code",
      "source": [
        "# prompt: extract transaction type, from_address, to_address, timestamp, date, indexed_at, format_amount_without_digit and format amout usd from new_var\n",
        "\n",
        "import pandas as pd\n",
        "\n",
        "# Assuming 'new_var' is the list of dictionaries you extracted from the API response\n",
        "\n",
        "def extract_transaction_data(new_var):\n",
        "    transaction_data = []\n",
        "\n",
        "    for transaction in new_var:\n",
        "\n",
        "        transaction_type = transaction.get('transaction_type')\n",
        "        from_address = transaction.get('from_address')\n",
        "        to_address = transaction.get('to_address')\n",
        "        timestamp = transaction.get('timestamp')\n",
        "        date = transaction.get('date')\n",
        "        indexed_at = transaction.get('indexed_at')\n",
        "        amount_btc = transaction.get('format_amount_without_digit')\n",
        "\n",
        "        if amount_btc:\n",
        "            amount_btc_no_digit = ''.join(filter(str.isalpha, str(amount_btc)))\n",
        "\n",
        "        amount_usd_formatted = transaction.get('format_amount_usd')\n",
        "\n",
        "        transaction_data.append({\n",
        "            'transaction_type': transaction_type,\n",
        "            'from_address': from_address,\n",
        "            'to_address': to_address,\n",
        "            'timestamp': timestamp,\n",
        "            'date': date,\n",
        "            'indexed_at': indexed_at,\n",
        "            'amount_btc_no_digit': amount_btc_no_digit,\n",
        "            'amount_usd_formatted': amount_usd_formatted,\n",
        "        })\n",
        "\n",
        "    return pd.DataFrame(transaction_data)\n",
        "\n",
        "# Example usage (assuming 'new_var' is defined from your previous code):\n",
        "# df_transactions = extract_transaction_data(new_var)\n",
        "# print(df_transactions)\n"
      ],
      "metadata": {
        "id": "iKp4wS4Kpyf9"
      },
      "execution_count": null,
      "outputs": []
